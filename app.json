{
  "expo": {
    "name": "Calculator",
    "description": "A zerozone calculator project.",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "sdkVersion": "27.0.0",
    "icon": "./assets/images/icon.png",
    "slug": "react-native-calculator",
    "privacy": "public",
    "orientation": "portrait",
    "primaryColor": "#ffffff",
    "loading": {
      "icon": "./assets/images/icon.png"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.raynstudios.reactcalculator",
      "supportsTablet": true
    },
    "android": {
      "package": "com.raynstudios.reactcalculator",
      "versionCode": 1
    }
  }
}
